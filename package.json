{
  "name": "latex-documentation-build",
  "version": "1.0.0",
  "description": "A latex skeleton and build, for seamlessly building out documentation time and time again. ",
  "main": "index.js",
  "scripts": {
    "build": "node build.js",
    "initial": "pdflatex main.tex",
    "start": "node watcher.js & open -a Skim main.pdf",
    "start-linux": "node watcher.js & evince main.pdf",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch-pdf": "wr -h"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build",
      "pre-push": "npm run build"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CharlieGreenman/latex-documentation.git"
  },
  "keywords": [
    "latex"
  ],
  "author": "Charlie Greenman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CharlieGreenman/latex-documentation/issues"
  },
  "homepage": "https://github.com/CharlieGreenman/latex-documentation#readme",
  "dependencies": {
    "@razroo/razroo-markdown-engine": "1.2.2",
    "glob": "^7.1.6",
    "husky": "^4.3.0",
    "mkdirp": "^1.0.4"
  },
  "devDependencies": {
    "chokidar": "^2.1.6",
    "glob": "^7.1.4",
    "jest": "^22.4.4",
    "node-sass": "^4.12.0",
    "sass-true": "^4.0.0",
    "shelljs": "^0.7.8"
  }
}
